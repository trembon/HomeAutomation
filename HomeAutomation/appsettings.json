{
  "ConnectionStrings": {
    "Default": "postgres connection string",
    "Json": "database.json"
  },
  "ScheduledJobs": {
    "CleanupLogScheduleJob": 86400,
    "ImportSunDataScheduleJob": 7200,
    "TriggerScheduledJob": 600
  },
  "Slack": {
    "Token": "token from slack"
  },
  "Telldus": {
    "APIUrl": "telldus url",
    "IgnoreDuplicateWebhooksInSeconds": 15
  },
  "ZWave": {
    "APIUrl": "zwave url"
  },
  "Tuya": {
    "APIUrl": "tuya url"
  },
  "Forecasts": {
    "SunDataUrl": "https://api.sunrise-sunset.org/json?lat={0}&lng={1}&formatted=0",
    "WeatherUrl": "https://www.yr.no/place/Sverige/Skåne/Malmö/forecast.xml",
    "Lat": 50.123456,
    "Lng": 15.654321
  },
  "SMTP": {
    "Enabled": true,
    "ServerName": "homeautomation",
    "Port": 25
  },
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Error",
        "HomeAutomation": "Information"
      }
    },
    "LogLevel": {
      "Default": "Information"
    }
  }
}