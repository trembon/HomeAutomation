{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/libs/signalr",
      "files": [
        "dist/browser/signalr.js"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/libs/jquery",
      "files": [
        "dist/jquery.js"
      ]
    },
    {
      "library": "bootstrap@4.5.3",
      "destination": "wwwroot/libs/bootstrap",
      "files": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    },
    {
      "library": "popper.js@1.16.1",
      "destination": "wwwroot/libs/popper",
      "files": [
        "dist/umd/popper.js"
      ]
    },
    {
      "library": "font-awesome-5-css@5.4.1",
      "destination": "wwwroot/libs/font-awesome",
      "files": [
        "css/fontawesome-all.css",
        "webfonts/*"
      ]
    }
  ]
}